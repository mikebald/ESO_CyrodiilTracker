<GuiXml>
	<Controls>
		<TopLevelControl name="CyrodiilTrackerUI" clampedToScreen="true" topmost="true" resizeToFitDescendents="true">
			<Anchor point="TOPLEFT" relativeTo="$(parent)" relativePoint="12"/>
			<OnMoveStop> CyroTracker.OnMoveStop() </OnMoveStop>
			<OnUpdate> CyroTracker.OnUpdate() </OnUpdate>
			<Controls>
				<Backdrop name="$(parent)_Backdrop" edgeColor="FFFFFF" centerColor="FFFFFF" alpha="0.2" >
					<AnchorFill/>
					<Edge edgeSize="1"/>
				</Backdrop>
				<Label name="$(parent)_lblPosition" font="CyrodiilTracker/FSEX300.ttf|16" color="000000" wrapMode="TRUNCATE"
				verticalAlignment="TOP" horizontalAlignment="LEFT" text="000000000">
					<Anchor point="TOPLEFT" relativeTo="$(parent)" relativePoint="TOPLEFT"/>
				</Label>
			</Controls>
		</TopLevelControl>
	</Controls>
</GuiXml>