<GuiXml>
	<Controls>
		<TopLevelControl name="CrownTrackerUI" clampedToScreen="true" topmost="true">
			<Anchor point="TOPLEFT" relativeTo="$(parent)" relativePoint="TOPLEFT"/>
			<Controls>
				<Backdrop name="$(parent)_Backdrop" edgeColor="FFFFFF" centerColor="FFFFFF" alpha="0" >
					<AnchorFill/>
					<Edge edgeSize="1"/>
				</Backdrop>
				<Label name="$(parent)_Label" font="ZoFontCreditsText" color="000000" wrapMode="TRUNCATE" 
				verticalAlignment="TOP" horizontalAlignment="LEFT" text="0000">
					<Anchor point="TOPLEFT" relativeTo="$(parent)" relativePoint="TOPLEFT"/>
				</Label>
			</Controls>
		</TopLevelControl>
	</Controls>
</GuiXml>